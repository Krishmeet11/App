{
  "functions": {
    "api/**/*.php": {
      "runtime": "vercel-php@0.7.4"
    }
  },
  "routes": [
    {
      "src": "/(.*).(css|js|png|jpg|jpeg|gif|svg)",
      "dest": "/$1.$2"
    },
    {
      "src": "/(.*)",
      "dest": "/api/submit.php"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}


